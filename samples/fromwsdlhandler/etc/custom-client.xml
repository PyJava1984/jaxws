<bindings 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    wsdlLocation="http://localhost:8080/jaxws-fromwsdlhandler/addnumbers?wsdl"
    xmlns="http://java.sun.com/xml/ns/jaxws">    
    <bindings node="wsdl:definitions">
        <package name="fromwsdlhandler.client"/>
    </bindings>
    <bindings node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://duke.org']">
        <jaxb:schemaBindings>
            <jaxb:package name="fromwsdlhandler.client"/>
        </jaxb:schemaBindings>
    </bindings>
    <bindings node="wsdl:definitions" xmlns:jws="http://java.sun.com/xml/ns/javaee">
        <jws:handler-chain>
            <jws:handler-chain>
                <jws:handler>
                    <jws:handler-class>fromwsdlhandler.common.LoggingHandler</jws:handler-class>
                </jws:handler>
            </jws:handler-chain>
        </jws:handler-chain>
    </bindings>
</bindings>

