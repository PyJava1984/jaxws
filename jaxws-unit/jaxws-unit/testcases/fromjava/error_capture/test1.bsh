// just the basics
try {
  myServicePort.foo();
  fail("exception expected");
} catch( javax.xml.ws.soap.SOAPFaultException e ) {
  e.printStackTrace(); // so that we can see it

  top = e.getCause().getCause().getStackTrace()[0];
  System.out.println(top);
  assertEquals(top.getMethodName(),"bar");

  top = e.getCause().getStackTrace()[0];
  System.out.println(top);
  assertEquals(top.getMethodName(),"foo");
}