<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bindings xmlns="http://java.sun.com/xml/ns/jaxws">
    
  <bindings xmlns:jws="http://java.sun.com/xml/ns/javaee">
    <jws:handler-chains>
    
      <jws:handler-chain>
        <jws:protocol-bindings>##SOAP11_HTTP</jws:protocol-bindings>
        <jws:handler>
          <jws:handler-class>fromwsdl.handler_simple.common.SOAPTestHandler</jws:handler-class>
        </jws:handler>
      </jws:handler-chain>
      
      <jws:handler-chain>
        <jws:protocol-bindings>##SOAP12_HTTP</jws:protocol-bindings>
        <jws:handler>
          <jws:handler-class>fromwsdl.handler_simple.client.Port12Handler</jws:handler-class>
        </jws:handler>
      </jws:handler-chain>
      
    </jws:handler-chains>
  </bindings>
  
</bindings>
